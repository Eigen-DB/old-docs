{
    "openapi": "3.0.0",
    "info": {
      "title": "Vector API - Search",
      "version": "1.0.0",
      "description": "API for searching similar vectors."
    },
    "paths": {
      "/vectors/search": {
        "post": {
          "summary": "Search for Similar Vectors",
          "operationId": "searchVectors",
          "requestBody": {
            "description": "Search for vectors similar to the query vector",
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "queryVectorId": {
                      "type": "string"
                    },
                    "k": {
                      "type": "integer"
                    }
                  },
                  "required": ["queryVectorId", "k"]
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Successfully retrieved similar vectors",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                }
              }
            },
            "400": {
              "description": "Invalid request body or error during search"
            }
          }
        }
      }
    },
    "components": {
      "schemas": {
        "VectorId": {
          "type": "string"
        }
      }
    }
  }
  